<div class="logo"> <img src="/images/logo.png" alt="logo" width="70%"></div>
<div class="pageBox">
<div class="mainPic">
    <img class='profilePic' src=" <%= currentFriend['avatarUrl'] %> " alt="profilepic">
</div>

<div class="friendDiv">
    
        <% currentFriend['friends'].forEach(friendId =>{ %>
            <% users.forEach(user => {  %> 
                <% if ( user['id'] === friendId ){ %> 
                    <% currentFriend = user%>
                    <div class="fb"> <a   href= "/friend/<%=user.id%>"> <img class="friendPic" src= " <%= user.avatarUrl %> " alt="avatar" > </a></div> 
            
                <% };});}) %>
    
</div>
</div>